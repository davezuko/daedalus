{
  "name": "daedalus",
  "version": "0.0.0",
  "description": "Utility library for writing performant functional JavaScript",
  "main": "src/daedalus.js",
  "scripts": {
    "clean": "rm -rf dist; mkdir dist",
    "webpack": "node ./node_modules/.bin/webpack ./src/daedalus.js",
    "webpack:prod": "node ./node_modules/.bin/webpack ./src/daedalus.js -p",
    "compile": "npm run webpack ./dist/daedalus.js; npm run webpack:prod ./dist/daedalus.min.js",
    "test": "node ./node_modules/karma/bin/karma start",
    "test:prod": "NODE_ENV=production npm run test",
    "deploy": "npm run clean; npm run compile; npm run test:prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davezuko/daedalus"
  },
  "keywords": [
    "functional",
    "js",
    "javascript"
  ],
  "author": "David Zukowski <david@zuko.me> (http://github.com/davezuko)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davezuko/daedalus/issues"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "jshint": "^2.7.0",
    "karma": "^0.12.32",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.5.0",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "phantomjs-polyfill": "0.0.1",
    "webpack": "^1.9.10"
  }
}
