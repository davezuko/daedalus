{
  "name": "redash",
  "version": "0.12.3",
  "description": "Compact library for writing performant functional JavaScript",
  "main": "dist/redash.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist/redash.js"
  ],
  "scripts": {
    "lint": "eslint src tests",
    "bundle": "rollup --format umd --name Redash -i src/redash.js -o dist/redash.js",
    "minify": "uglifyjs dist/redash.js --compress --mangle -o dist/redash.min.js",
    "compile": "npm run bundle && npm run minify",
    "test": "ava",
    "deploy": "npm run compile && npm run test"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "ISC",
  "ava": {
    "files": [
      "tests/a*.js",
      "tests/c*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "tap": false,
    "require": []
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.13.1",
    "lodash": "^4.13.1",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "ramda": "^0.21.0",
    "rollup": "^0.21.0",
    "uglify-js": "^2.6.1"
  }
}
